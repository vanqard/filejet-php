{
    "name": "vanqard/filejet-php",
    "license": "MIT",
    "type": "library",
    "description": "PHP library for communicating with FileJet service. FileJet handles upload of files to remote storage and is capable of simple image mutations.",
    "autoload": {
        "psr-4": {
            "FileJet\\": "src/"
        }
    },
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "psr/http-message": "^1.0",
        "psr/http-factory": "^1.0",
        "psr-discovery/http-client-implementations": "^1.0",
        "psr-discovery/http-factory-implementations": "^1.0"
    },
    "require-dev": {
        "guzzlehttp/psr7": "^1.0",
        "phpunit/phpunit": "^10",
        "psr-mock/http-client-implementation": "^1.0",
        "psr-mock/http-factory-implementation": "^1.0"
    },
    "suggest": {
        "guzzlehttp/guzzle": "HTTP client which conforms to PSR 18 http client specification",
        "guzzlehttp/psr7": "PSR7 compatible message factory"
    },
    "scripts": {
        "test": "phpunit tests"
    }
}
